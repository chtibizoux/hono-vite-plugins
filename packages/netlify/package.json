{
  "name": "@hono/vite-netlify",
  "description": "Vite plugin to build your Hono app for Netlify",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "test": "vitest --run",
    "build": "rimraf dist && tsup && publint",
    "watch": "tsup --watch",
    "prerelease": "yarn build",
    "release": "yarn publish"
  },
  "files": [
    "dist"
  ],
  "exports": {
    "./functions": {
      "types": "./dist/functions/index.d.ts",
      "import": "./dist/functions/index.js"
    },
    "./edge-functions": {
      "types": "./dist/edge-functions/index.d.ts",
      "import": "./dist/edge-functions/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "functions": [
        "./dist/functions/index.d.ts"
      ],
      "edge-functions": [
        "./dist/edge-functions/index.d.ts"
      ]
    }
  },
  "author": "Cl√©ment Songis <contact@clement-songis.cf> (https://github.com/chtibizoux)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/honojs/vite-plugins.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "homepage": "https://github.com/honojs/vite-plugins",
  "devDependencies": {
    "glob": "^10.3.10",
    "hono": "^4.2.7",
    "publint": "^0.1.12",
    "rimraf": "^5.0.1",
    "tsup": "^7.2.0",
    "vite": "^5.2.10",
    "vitest": "^1.2.1"
  },
  "peerDependencies": {
    "hono": "*"
  },
  "engines": {
    "node": ">=18.14.1"
  }
}
